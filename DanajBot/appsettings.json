{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        },
        "Console": {
            "FormatterName": "simple",
            "FormatterOptions": {
                "SingleLine": true,
                "IncludeScopes": true,
                "TimestampFormat": "yyyy-MM-dd HH:mm:ss.fff "
            }
        }
    },
    "AllowedHosts": "*",
    "AppSettings": {
        "DiscordToken": "",
        "BotChatChannelId": 0,
        "Zkouska": {
            "SourceChannelId": 0,
            "DestinationChannelId": 0
        },
        "EveryonePermissionChecksSettings": {
            "VerificationCategoryId": 0,
            "PermissionCheckIntervalMinutes": 60
        }
    }
}
